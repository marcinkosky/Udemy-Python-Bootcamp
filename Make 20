def makes_twenty(a,b):
    '''
    Docstring: Make 20 (Python Udemy Bootcamp excercise)
    Input: Two integers
    Output: Given two integers, return True if the sum of the integers is 20 or if one of the integers is 20. If not, return False
    '''

    if a + b == 20:
        return True
    else:
        return False


s1 = makes_twenty(20,10)
s2 = makes_twenty(12,8)
s3 = makes_twenty(2,3)

print(s1)
print(s2)
print(s3)
