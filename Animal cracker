def animal_crackers(n):
    '''
    Docstring: Animal crackers (Python Udemy Bootcamp excercise)
    Input: Two strings
    Output: Write a function takes a two-word string and returns True if both words begin with same letter
    '''
    
    splitted = n.split(" ")
    a = splitted[0]
    b = splitted[1]
    
    if a[0] == b[0]:
        return True
    else:
        return False

s1 = animal_crackers('Levelheaded Llama')
s2 = animal_crackers('Crazy Kangaroo')

print(s1)
print(s2)
