def almost_there(n):
    '''
    Docstring: Almost there (Python Udemy Bootcamp excercise)
    Input: An integer
    Output: Given an integer n, return True if n is within 10 of either 100 or 200
    '''
    if n < 210 and n > 190 and n < 110 and n > 90:
        return True
    else:
        return False

s1 = almost_there(209)
print(s1)
